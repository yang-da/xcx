view{
  box-sizing: border-box;
}
.container{
  padding: 35rpx 20rpx;
}
.info{
  width: 100%;
  background: #fff;
  border-radius: 10rpx;
  padding: 0rpx 30rpx 20rpx;
}
.h1{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  color: #333;
  font-weight: 700;
  position: relative;
}
.h1::before{
  content: '';
  display: inline-block;
  width:6rpx;
  height:32rpx;
  background: #333;
  position: absolute;
  left: -20rpx;
  top: 50%;
  transform: translateY(-50%);
}
.info view:nth-child(2){
  font-size:30rpx;
  color: #333;
}
.info view:nth-child(2) text{
  font-size: 26rpx;
  color: #A2A2A2;
  margin-left: 20rpx;
}
.info .addtress{
  color: #333;
  line-height: 38rpx;
  font-size: 26rpx;
  margin-top: 13rpx;
}

.wlInfo{
  width: 100%;
  background: #fff;
  border-radius: 10rpx;
  padding: 0rpx 30rpx 5rpx;
  margin-top: 20rpx;
}
