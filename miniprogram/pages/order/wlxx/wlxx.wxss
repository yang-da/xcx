.num{
  border-bottom: 1px solid #E4E4E4;
}
.num,.company{
  display: flex;
  height: 90rpx;
  line-height: 90rpx;
  position: relative;
}
.company .van-icon{
  position: absolute;
  right: 0;
}
.wlInfo label{
  font-size:28rpx;
  color: #333;
}
.wlInfo input{
  font-size: 28rpx;
  margin-left: 30rpx;
  margin-top: 24rpx;
  width: 70%;
}
picker{
  width: 70%;
}
.picker{
  font-size: 28rpx;
  margin-left: 30rpx;
}

.p{
  width: 100%;
  font-size: 24rpx;
  color: #A2A2A2;
  margin-top: 12rpx;
  padding-left: 30rpx;
}
.submit{
  margin-top: 50rpx;
  background-color: #E4E4E4;
  height:80rpx;
  border-radius:40rpx;
  line-height: 80rpx;
  text-align: center;
  width: 88%;
  font-size:32rpx;
  color: #fff;
}
.submit.active{
  background-color: #FF653C;
  color: #FFFFFF;
}

























