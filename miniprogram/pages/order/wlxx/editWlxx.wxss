@import "wlxx.wxss";

.wlInfo{
  position: relative;
}
.num,.company{
  width: 100%;
}
.edit{
  position: absolute;
  width: 120rpx;
  height: 86rpx;
  text-align: center;
  font-size:20rpx;
  color: #a2a2a2;
  right: 0;
  top: 46%;
  background-color: #fff;
  padding-left: 30rpx;
}
.edit::after{
  content: '';
  position: absolute;
  height: 100%;
  border-left: 1px solid #E4E4E4;
  top: 50%;
  transform: translateY(-50%);
  left: 30rpx;
}
.edit image{
  width: 28rpx;
  height: 28rpx;
  display: block;
  margin: 8rpx auto;
}

















